{
  "name": "re-app",
  "version": "0.2.5",
  "description": "react + redux frontend codebase",
  "main": "lib/index.js",
  "repository": "https://github.com/stackscz/re-app",
  "engines": {
    "node": "^4.0.0",
    "npm": "^3.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "ab": "node ./node_modules/re-app-builder",
    "build": "node ./node_modules/babel-cli/bin/babel src/ --out-dir lib --copy-files",
    "build:watch": "node ./node_modules/babel-cli/bin/babel src/ --out-dir lib --watch --copy-files",
    "test": "NODE_ENV=test babel-node ./node_modules/tape/bin/tape ./tests/**/*.js | node ./node_modules/tap-spec/bin/cmd.js",
    "test:file": "NODE_ENV=test babel-node ./node_modules/tape/bin/tape",
    "prepublish": "npm run build"
  },
  "author": "@stackscz",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-plugin-typecheck": "^3.8.0",
    "babel-plugin-webpack-alias": "^1.1.1",
    "bootstrap": "^3.3.6",
    "eslint-plugin-react": "^5.1.1",
    "marked": "^0.3.5",
    "pouchdb": "^5.3.2",
    "prismjs": "^1.4.1",
    "re-app-builder": "^0.2.6",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "redux-immutable-state-invariant": "^1.2.3",
    "relational-pouch": "^1.4.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "webpack-node-externals": "^1.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "denormalizr": "0.0.8",
    "dot-object": "^1.4.1",
