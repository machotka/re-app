    "immutability-helper": "^2.0.0",
    "invariant": "^2.2.1",
    "lodash": "^4.5.0",
    "moment": "^2.12.0",
    "normalizr": "^2.0.1",
    "object-hash": "^1.1.2",
    "perfect-scrollbar": "^0.6.11",
    "qs": "^6.1.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-helmet": "^3.1.0",
    "react-json-inspector": "^6.1.3",
    "react-pure-render": "^1.0.2",
    "react-quill": "^0.4.1",
    "react-redux": "^4.4.0",
