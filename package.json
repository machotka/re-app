{
  "name": "re-app",
  "version": "0.16.4",
  "description": "react + redux frontend codebase",
  "main": "lib/index.js",
  "repository": "https://github.com/stackscz/re-app",
  "engines": {
    "node": "^4.0.0",
    "npm": "^3.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "ab": "node ./node_modules/re-app-builder",
    "build": "node ./node_modules/babel-cli/bin/babel src/ --out-dir lib --copy-files",
    "build:watch": "node ./node_modules/babel-cli/bin/babel src/ --out-dir lib --watch --copy-files",
    "lint": "eslint src/",
    "test": "NODE_ENV=test mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "test:file": "npm test -- --grep",
    "test:file:watch": "npm test -- --watch --grep",
    "prepublish": "npm run build"
  },
  "author": "@stackscz",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-webpack-alias": "^1.1.1",
    "bootstrap": "^3.3.6",
    "eslint": "^2.13.1",
    "eslint-config-stacks": "0.0.3",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "expect": "^1.20.1",
    "marked": "^0.3.5",
    "mocha": "^2.5.3",
    "prismjs": "^1.4.1",
    "re-app-builder": "^0.5.0",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "redux-immutable-state-invariant": "^1.2.3",
    "webpack-node-externals": "^1.0.0"
  },
  "dependencies": {
    "history": "^1.17.0",
    "react-json-inspector": "^7.0.0",
    "redux-logger": "^2.6.1",
    "react-modal2": "^3.0.2",
    "react-portal": "^2.1.3",
    "redux-batched-subscribe": "^0.1.5",
    "validate.js": "^0.9.0"
  },
  "peerDependencies": {
    "classnames": "^2.2.3",
    "denormalizr": "0.0.8",
    "dot-object": "^1.4.1",
    "fbjs": "^0.8.2",
    "invariant": "^2.2.1",
    "lodash": "^4.5.0",
    "moment": "^2.12.0",
    "normalizr": "^2.0.1",
    "object-hash": "^1.1.2",
    "qs": "^6.1.0",
    "react": "^15.1.0",
    "react-addons-css-transition-group": "^15.1.0",
    "react-dom": "^15.0.1",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.5.1",
    "redux-form": "^6.0.0-alpha.15",
    "redux-saga": "^0.10.3",
    "seamless-immutable": "^6.1.0",
    "tcomb": "^3.0.0",
    "tcomb-validation": "^3.0.0"
  },
  "npmName": "re-app"
}
